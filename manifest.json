{
  "manifest_version": 2,
  "name": "Task to Air",
  "version": "0.1",
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "tabs", "<all_urls>", "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
